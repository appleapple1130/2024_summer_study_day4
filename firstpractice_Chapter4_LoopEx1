package firstpractice;

public class Chapter4_LoopEx1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int dan;
		int times;
		for(dan=2;dan<=9;dan++) {
			if(dan%2==1)
				continue;
			for (times=1;times<=9;times++) {
				System.out.println(dan+"X"+times+"="+dan*times);
			}
			System.out.println( );
		}
	}

}
